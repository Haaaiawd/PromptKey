{
  "productName": "prompt-manager", 
  "version": "0.1.0",
  "identifier": "com.prompt-manager.app",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "./src"
  },
  "app": {
    "windows": [
      {
        "label": "main",
